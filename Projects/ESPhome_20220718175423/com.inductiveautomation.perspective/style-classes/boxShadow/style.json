{
  "base": {
    "style": {
      "boxShadow": "1px 2px 3px 4px rgba(20,20,20,0.4);",
      "cursor": "pointer"
    }
  },
  "variants": [
    {
      "pseudo": "hover",
      "animation": {
        "duration": "100ms",
        "iterationCount": "1",
        "keyframes": {
          "0%": {
            "boxShadow": "1px 2px 3px 4px rgba(20,20,20,0.4);"
          },
          "100%": {
            "boxShadow": "0px 1px 1px 2px rgba(20,20,20,0.4);"
          }
        }
      }
    },
    {
      "pseudo": "active",
      "animation": {
        "duration": "200ms",
        "iterationCount": "1",
        "keyframes": {
          "0%": {
            "boxShadow": "1px 2px 3px 4px rgba(20,20,20,0.4);"
          },
          "100%": {}
        }
      }
    }
  ]
}